{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "max-len": [
      "error",
      140
    ],
    "require-jsdoc": [
      "off"
    ],
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "space-infix-ops": "error"
  }
}